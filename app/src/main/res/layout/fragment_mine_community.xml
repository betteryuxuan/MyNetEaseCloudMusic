<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/dark_white"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.minepage.view.MineCommunityFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            android:id="@+id/item_mine_layout"
            layout="@layout/item_note_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/note_star_card_1" />

        <View
            android:id="@+id/note_mine_view_2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/white_gray"
            app:layout_constraintTop_toBottomOf="@id/item_note_layout" />

        <include
            android:id="@+id/item_mine_layout_2"
            layout="@layout/item_note_layout_2"

            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </LinearLayout>
</androidx.core.widget.NestedScrollView>
